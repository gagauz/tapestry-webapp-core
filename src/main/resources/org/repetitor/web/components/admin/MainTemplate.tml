	<script type="text/javascript">
		window.$j = jQuery.noConflict();
        ;(function ($, sS, menu) {
            if (!sS.getItem('current')) {
                sS.setItem('current', "menu0");
            }
            function checker() {
                menu.removeClass('active open');
                $('#' + sS.getItem('current')).addClass('active').children('a').children('.arrow').addClass('open');
            }
            if (sS.getItem('current')) {
                checker();
            }
            $('.brand').on('click', function () {
                sS.clear();
            });
            return (function menus() {
                menu.on('click', function () {
                    var $that = $(this);
                    sS.setItem('current', $that.attr('id'));
                });
            })();
        })(jQuery, sessionStorage, jQuery('.page-sidebar-menu li'));
	</script>
                 
